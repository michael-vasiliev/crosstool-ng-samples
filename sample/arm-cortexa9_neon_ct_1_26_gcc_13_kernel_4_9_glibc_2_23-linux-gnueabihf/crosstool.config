CT_CONFIG_VERSION="4"
CT_EXPERIMENTAL=y
CT_ARCH_ARM=y
CT_ARCH_CPU="cortex-a9"
CT_ARCH_FPU="neon"
CT_ARCH_FLOAT_HW=y
CT_TARGET_VENDOR="cortexa9_neon_ct_1_26_gcc_13_kernel_4_9_glibc_2_23"
CT_TOOLCHAIN_ENABLE_NLS=y
CT_KERNEL_LINUX=y
CT_BINUTILS_LINKER_LD_GOLD=y
CT_BINUTILS_GOLD_THREADS=y
CT_BINUTILS_LD_WRAPPER=y
CT_BINUTILS_PLUGINS=y
CT_LIBC_GLIBC=y
CT_GLIBC_USE_LIBIDN_ADDON=y
CT_GLIBC_LOCALES=y
CT_GLIBC_KERNEL_VERSION_NONE=y
# CT_CC_GCC_ENABLE_TARGET_OPTSPACE is not set
CT_CC_LANG_CXX=y

# vms
CT_DEBUG_GDB=n
# vms
CT_LINUX_V_4_9=y
# vms
CT_GLIBC_V_2_23=y
#
VMS_TARGET_VENDOR="cortexa9_neon"
# vms
CT_GCC_V_13=y
